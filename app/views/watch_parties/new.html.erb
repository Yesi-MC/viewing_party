<%= form_with model: @watch_party, url: watch_parties_path, method: :post do |f| %>
  <h1>Movie: <%= session[:title] %></h1>
  
  <%= f.label :date %>
  <%= f.text_field :date %><br/>
  <%= f.label :time %>
  <%= f.text_field :time%>

  <%= f.label :duration %>
  <%= f.text_field :duration , id: "runtime" %><br/>


<% end %>